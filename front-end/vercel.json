{
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/images/(.*)", "destination": "/images/$1" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
