# Configuration .env pour SpotBulle Production

# URL du backend avec le bon préfixe API
VITE_API_BASE_URL=https://spotbulle-backend-0lax.onrender.com/api/v1

# Configuration de l'environnement
NODE_ENV=production
VITE_APP_NAME=SpotBulle
VITE_APP_VERSION=1.0.0

# Configuration de sécurité (optionnel)
VITE_ENABLE_LOGS=true
VITE_API_TIMEOUT=30000

# Taille maximale des fichiers (en octets) - 200 Mo
VITE_MAX_FILE_SIZE=209715200
